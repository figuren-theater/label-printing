{
    "name": "figuren-theater/label-printing",
    "description": "Create printable labels with blocks.",
    "license": "GPL-3.0+",
    "type": "wordpress-plugin",
    "keywords": [
        "wordpress",
        "wordpress-plugin",
        "Theater",
        "Gutenberg",
        "Blocks",
        "Label-Printing",
        "print.css"
    ],
    "authors": [
        {
            "name": "figuren.theater",
            "email": "info@figuren.theater"
        },
        {
            "name": "Carsten Bach",
            "email": "mail@carsten-bach.de",
            "homepage": "https://carsten-bach.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://websites.fuer.figuren.theater",
    "support": {
        "email": "info@figuren.theater",
        "issues": "https://github.com/figuren-theater/label-printing/issues",
        "source": "https://github.com/figuren-theater/label-printing"
    },
    "require-dev": {
        "figuren-theater/code-quality": "*"
    },
    "autoload": {
        "classmap": [
            "inc/"
        ],
        "files": [
            "inc/namespace.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "koodimonni/composer-dropin-installer": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
